{
  "name": "StackOverflow Dark",
  "author": "dieghernan",
  "semanticHighlighting": true,
  "type": "dark",
  "tokenColors": [
    {
      "settings": {
        "background": "#1D1B1B",
        "foreground": "#FFFFFF"
      }
    },
    {
      "name": "hljs-comment",
      "scope": "comment",
      "settings": {
        "foreground": "#999999"
      }
    },
    {
      "name": "hljs-keyword",
      "scope": [
        "keyword.control",
        "keyword.operator.expression",
        "keyword.operator.new",
        "keyword.other",
        "storage.modifier",
        "storage.type",
        "support.variable"
      ],
      "settings": {
        "foreground": "#90C4F9"
      }
    },
    {
      "name": "hljs-selector-tag",
      "scope": [
        "entity.name.tag.css",
        "entity.name.tag.scss"
      ],
      "settings": {
        "foreground": "#90C4F9"
      }
    },
    {
      "name": "hljs-meta",
      "scope": [
        "entity.name.tag.doctype.html",
        "keyword.control.flow.alias.yaml",
        "keyword.control.flow.anchor.yaml",
        "keyword.other.important",
        "meta.doctype",
        "meta.tag.metadata.doctype",
        "meta.tag.metadata.doctype.html",
        "punctuation.section.embedded",
        "punctuation.section.embedded.begin.php",
        "punctuation.section.embedded.end.php",
        "variable.other.alias.yaml",
        "variable.other.anchor.yaml"
      ],
      "settings": {
        "foreground": "#90C4F9"
      }
    },
    {
      "name": "hljs-section",
      "scope": "markup.heading",
      "settings": {
        "foreground": "#90C4F9"
      }
    },
    {
      "name": "hljs-attr",
      "scope": [
        "entity.name.tag.yaml",
        "entity.other.attribute-name",
        "support.type.property-name"
      ],
      "settings": {
        "foreground": "#90C4F9"
      }
    },
    {
      "name": "hljs-attribute",
      "scope": [
        "support.type.property-name.css",
        "support.type.property-name.media.css"
      ],
      "settings": {
        "foreground": "#DF9FD9"
      }
    },
    {
      "name": "hljs-name",
      "scope": "entity.name.tag.html",
      "settings": {
        "foreground": "#FB9D51"
      }
    },
    {
      "name": "hljs-type",
      "scope": [
        "storage.type.java",
        "storage.type.primitive",
        "storage.type.tag"
      ],
      "settings": {
        "foreground": "#FB9D51"
      }
    },
    {
      "name": "hljs-number",
      "scope": [
        "constant.numeric",
        "constant.other.color",
        "keyword.other.unit"
      ],
      "settings": {
        "foreground": "#FB9D51"
      }
    },
    {
      "name": "hljs-selector-id",
      "scope": "entity.other.attribute-name.id",
      "settings": {
        "foreground": "#FB9D51"
      }
    },
    {
      "name": "hljs-quote",
      "scope": "markup.quote",
      "settings": {
        "foreground": "#FB9D51"
      }
    },
    {
      "name": "hljs-selector-class",
      "scope": "entity.other.attribute-name.class.css",
      "settings": {
        "foreground": "#90C4F9"
      }
    },
    {
      "name": "hljs-string",
      "scope": [
        "keyword.other.directive.yaml",
        "string"
      ],
      "settings": {
        "foreground": "#B6BE6A"
      }
    },
    {
      "name": "hljs-regexp",
      "scope": "string.regexp",
      "settings": {
        "foreground": "#B6BE6A"
      }
    },
    {
      "name": "hljs-symbol",
      "scope": [
        "constant.language.symbol",
        "constant.other.reference.link.markdown",
        "constant.other.symbol"
      ],
      "settings": {
        "foreground": "#B6BE6A"
      }
    },
    {
      "name": "hljs-variable",
      "scope": [
        "variable.language",
        "variable.language.this.php",
        "variable.other.constant",
        "variable.other.php",
        "variable.other.readwrite.instance.ruby",
        "variable.scss"
      ],
      "settings": {
        "foreground": "#B6BE6A"
      }
    },
    {
      "name": "hljs-link",
      "scope": [
        "markup.link",
        "markup.underline.link"
      ],
      "settings": {
        "foreground": "#B6BE6A"
      }
    },
    {
      "name": "hljs-selector-pseudo",
      "scope": "entity.other.attribute-name.pseudo-element.css",
      "settings": {
        "foreground": "#90C4F9"
      }
    },
    {
      "name": "hljs-built_in",
      "scope": "support.function",
      "settings": {
        "foreground": "#FB9D51"
      }
    },
    {
      "name": "hljs-title",
      "scope": [
        "entity.name.type",
        "support.class"
      ],
      "settings": {
        "foreground": "#FB9D51"
      }
    },
    {
      "name": "hljs-literal",
      "scope": "constant.language",
      "settings": {
        "foreground": "#FB9D51"
      }
    },
    {
      "name": "hljs-bullet",
      "scope": "punctuation.definition.list.begin",
      "settings": {
        "foreground": "#CCCCCC"
      }
    },
    {
      "name": "hljs-code",
      "scope": [
        "markup.inline.raw",
        "markup.raw"
      ],
      "settings": {
        "foreground": "#CCCCCC"
      }
    },
    {
      "name": "hljs-deletion",
      "scope": "invalid",
      "settings": {
        "foreground": "#FCC5C5"
      }
    },
    {
      "name": "hljs-emphasis",
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "hljs-strong",
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold"
      }
    }
  ],
  "colors": {
    "activityBar.activeBackground": "#493526",
    "activityBar.background": "#211E1C",
    "activityBar.foreground": "#FB9D51",
    "activityBar.inactiveForeground": "#999999",
