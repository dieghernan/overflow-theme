{
  "name": "StackOverflow Light",
  "author": "dieghernan",
  "semanticHighlighting": true,
  "type": "light",
  "tokenColors": [
    {
      "settings": {
        "background": "#F6F6F6",
        "foreground": "#0C0D0E"
      }
    },
    {
      "name": "hljs-comment",
      "scope": "comment",
      "settings": {
        "foreground": "#666F78"
      }
    },
    {
      "name": "hljs-keyword",
      "scope": [
        "keyword.control",
        "keyword.operator.expression",
        "keyword.operator.new",
        "keyword.other",
        "storage.modifier",
        "storage.type",
        "support.variable"
      ],
      "settings": {
        "foreground": "#015493"
      }
    },
    {
      "name": "hljs-selector-tag",
      "scope": [
        "entity.name.tag.css",
        "entity.name.tag.scss"
      ],
      "settings": {
        "foreground": "#015493"
      }
    },
    {
      "name": "hljs-meta",
      "scope": [
        "entity.name.tag.doctype.html",
        "keyword.control.flow.alias.yaml",
        "keyword.control.flow.anchor.yaml",
        "keyword.other.important",
        "meta.doctype",
        "meta.tag.metadata.doctype",
        "meta.tag.metadata.doctype.html",
        "punctuation.section.embedded",
        "punctuation.section.embedded.begin.php",
        "punctuation.section.embedded.end.php",
        "variable.other.alias.yaml",
        "variable.other.anchor.yaml"
      ],
      "settings": {
        "foreground": "#015493"
      }
    },
    {
      "name": "hljs-section",
      "scope": "markup.heading",
      "settings": {
        "foreground": "#015493"
      }
    },
    {
      "name": "hljs-attr",
      "scope": [
        "entity.name.tag.yaml",
        "entity.other.attribute-name",
        "support.type.property-name"
      ],
      "settings": {
        "foreground": "#015493"
      }
    },
    {
      "name": "hljs-attribute",
      "scope": [
        "support.type.property-name.css",
        "support.type.property-name.media.css"
      ],
      "settings": {
        "foreground": "#803378"
      }
    },
    {
      "name": "hljs-name",
      "scope": "entity.name.tag.html",
      "settings": {
        "foreground": "#B75301"
      }
    },
    {
      "name": "hljs-type",
      "scope": [
        "storage.type.java",
        "storage.type.primitive",
        "storage.type.tag"
      ],
      "settings": {
        "foreground": "#B75301"
      }
    },
    {
      "name": "hljs-number",
      "scope": [
        "constant.numeric",
        "constant.other.color",
        "keyword.other.unit"
      ],
      "settings": {
        "foreground": "#B75301"
      }
    },
    {
      "name": "hljs-selector-id",
      "scope": "entity.other.attribute-name.id",
      "settings": {
        "foreground": "#B75301"
      }
    },
    {
      "name": "hljs-quote",
      "scope": "markup.quote",
      "settings": {
        "foreground": "#B75301"
      }
    },
    {
      "name": "hljs-selector-class",
      "scope": "entity.other.attribute-name.class.css",
      "settings": {
        "foreground": "#015493"
      }
    },
    {
      "name": "hljs-string",
      "scope": [
        "keyword.other.directive.yaml",
        "string"
      ],
      "settings": {
        "foreground": "#567A0D"
      }
    },
    {
      "name": "hljs-regexp",
      "scope": "string.regexp",
      "settings": {
        "foreground": "#567A0D"
      }
    },
    {
      "name": "hljs-symbol",
      "scope": [
        "constant.language.symbol",
        "constant.other.reference.link.markdown",
        "constant.other.symbol"
      ],
      "settings": {
        "foreground": "#567A0D"
      }
    },
    {
      "name": "hljs-variable",
      "scope": [
        "variable.language",
        "variable.language.this.php",
        "variable.other.constant",
        "variable.other.php",
        "variable.other.readwrite.instance.ruby",
        "variable.scss"
      ],
      "settings": {
        "foreground": "#567A0D"
      }
    },
    {
      "name": "hljs-link",
      "scope": [
        "markup.link",
        "markup.underline.link"
      ],
      "settings": {
        "foreground": "#567A0D"
      }
    },
    {
      "name": "hljs-selector-pseudo",
      "scope": "entity.other.attribute-name.pseudo-element.css",
      "settings": {
        "foreground": "#015493"
      }
    },
    {
      "name": "hljs-built_in",
      "scope": "support.function",
      "settings": {
        "foreground": "#B75301"
      }
    },
    {
      "name": "hljs-title",
      "scope": [
        "entity.name.type",
        "support.class"
      ],
      "settings": {
        "foreground": "#B75301"
      }
    },
    {
      "name": "hljs-literal",
      "scope": "constant.language",
      "settings": {
        "foreground": "#B75301"
      }
    },
    {
      "name": "hljs-bullet",
      "scope": "punctuation.definition.list.begin",
      "settings": {
        "foreground": "#3B4045"
      }
    },
    {
      "name": "hljs-code",
      "scope": [
        "markup.inline.raw",
        "markup.raw"
      ],
      "settings": {
        "foreground": "#3B4045"
      }
    },
    {
      "name": "hljs-deletion",
      "scope": "invalid",
      "settings": {
        "foreground": "#9C2121"
      }
    },
    {
      "name": "hljs-emphasis",
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "hljs-strong",
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold"
      }
    }
  ],
  "colors": {
    "activityBar.activeBackground": "#E9D5C5",
    "activityBar.background": "#F5F3F1",
    "activityBar.foreground": "#B75301",
    "activityBar.inactiveForeground": "#666F78",
    "activityBarBadge.background": "#B75301",
    "activityBarBadge.foreground": "#F6F6F6",
    "badge.background": "#B75301",
    "badge.foreground": "#F6F6F6",
    "breadcrumb.activeSelectionForeground": "#0C0D0E",
    "breadcrumb.background": "#DFDFDF",
    "breadcrumb.focusForeground": "#0C0D0E",
    "breadcrumb.foreground": "#666F78",
    "button.background": "#B75301",
